    "# https://www.acmicpc.net/problem/11399\n",
    "def solution(n, a):\n",
    "    a.sort()\n",
    "    res = [0] * n\n",
    "    res[0] = a[0]\n",
    "    \n",
    "    for i in range(1,n):\n",
    "        res[i] = res[i-1] + a[i]\n",
    "    \n",
    "    return sum(res)\n",
    "\n",
    "\n",
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "print(solution(n, a))"
   ]
