    "def dfs(l, s):\n",
    "    if l == m:\n",
    "        print(*res)\n",
    "        return\n",
    "    else:\n",
    "        for i in range(s+1, n):\n",
    "            res[l] = arr[i]\n",
    "            dfs(l+1, s)\n",
    "            s+=1\n",
    "\n",
    "n, m = map(int, input().split())\n",
    "arr = [i for i in range(1,n+1)]\n",
    "res = [0] * m\n",
    "dfs(0, -1)"
   ]
