   "source": [
    "# https://www.acmicpc.net/problem/3107\n",
    "s = input()\n",
    "ip = s.split(':')\n",
    "if ip[0] == '':\n",
    "    ip = ip[1:]\n",
    "elif ip[-1] == '':\n",
    "    ip = ip[:-1]\n",
    "\n",
    "res = ''\n",
    "for i in ip:\n",
    "    if i == '':\n",
    "        res += '0000:'* (8-len(ip)+1)\n",
    "    else:\n",
    "        res += i.zfill(4)+':'\n",
    "\n",
    "print(res[:-1])"
   ]
  }
