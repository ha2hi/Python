    "# https://www.acmicpc.net/problem/2212\n",
    "def solution(n, k, a):\n",
    "    a.sort()\n",
    "    diff = []\n",
    "    for i in range(n-1):\n",
    "        diff.append(a[i+1]-a[i])\n",
    "    \n",
    "    return sum(sorted(diff)[:n-k])\n",
    "\n",
    "n = int(input())\n",
    "k = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "print(solution(n, k, a))"
   ]
