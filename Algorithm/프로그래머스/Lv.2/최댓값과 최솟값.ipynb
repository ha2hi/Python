    "# https://school.programmers.co.kr/learn/courses/30/lessons/12939\n",
    "def solution(s):\n",
    "    a = s.split()\n",
    "    a = list(map(int, a))\n",
    "    return f\"{min(a)} {max(a)}\"\n",
    "\n",
    "\n",
    "s = \"-1 -2 -3 -4\"\n",
    "solution(s)"
