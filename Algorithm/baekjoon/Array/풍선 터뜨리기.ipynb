   "source": [
    "from collections import deque\n",
    "n = int(input())\n",
    "q = deque(enumerate(map(int, input().split())))\n",
    "res = []\n",
    "\n",
    "while q:\n",
    "    k, v = q.popleft()\n",
    "    res.append(str(k+1))\n",
    "\n",
    "    if v > 0:\n",
    "        q.rotate(-(v-1))\n",
    "    elif v < 0:\n",
    "        q.rotate(-v)\n",
    "\n",
    "print(' '.join(res))"
   ]
