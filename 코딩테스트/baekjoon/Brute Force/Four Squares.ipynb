   "source": [
    "# https://www.acmicpc.net/problem/17626\n",
    "n = int(input())\n",
    "dp = [0,1]\n",
    "\n",
    "for i in range(2, n+1):\n",
    "    if (i**0.5)%1 == 0:\n",
    "        dp.append(1)\n",
    "        continue\n",
    "    tmp = 4\n",
    "    j = 1\n",
    "    while (j**2) <= i:\n",
    "        tmp = min(tmp, dp[i-j**2])\n",
    "        j += 1\n",
    "    dp.append(tmp+1)\n",
    "\n",
    "print(dp[n])"
   ]
