    "def DFS(v, water):\n",
    "    if v == k:\n",
    "        res.add(water)\n",
    "    else:\n",
    "        for i in range(k):\n",
    "            DFS(v+1, water-a[i])\n",
    "            DFS(v+1, water)\n",
    "k = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "res = set()\n",
    "DFS(0, sum(a))"
   ]
