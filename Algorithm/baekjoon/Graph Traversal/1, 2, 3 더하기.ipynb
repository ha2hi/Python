    "# https://www.acmicpc.net/problem/9095\n",
    "def dfs(s):\n",
    "    global res\n",
    "    if s > n:\n",
    "        return\n",
    "    if s == n:\n",
    "        res += 1\n",
    "    else:\n",
    "        dfs(s+1)\n",
    "        dfs(s+2)\n",
    "        dfs(s+3)\n",
    "\n",
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    res = 0\n",
    "    dfs(0)\n",
    "    print(res)"
   ]
