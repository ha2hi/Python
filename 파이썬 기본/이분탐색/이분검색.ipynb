   "source": [
    "n, m = map(int, input().split())\n",
    "a = list(map(int, input().split()))\n",
    "lt = 0\n",
    "rt = n - 1\n",
    "a.sort()\n",
    "while lt <= rt:\n",
    "    mid = (lt+rt) // 2\n",
    "    if m > a[mid]:\n",
    "        lt = mid + 1\n",
    "    elif m < a[mid]:\n",
    "        rt = mid - 1\n",
    "    else:\n",
    "        print(mid+1)\n",
    "        break\n"
   ]
