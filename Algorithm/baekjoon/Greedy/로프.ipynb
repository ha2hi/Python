    "# https://www.acmicpc.net/problem/2217\n",
    "def solution(N):\n",
    "    res = 0\n",
    "    for x in a:\n",
    "        tmp = x*N\n",
    "        res = max(res, tmp)\n",
    "        N -= 1\n",
    "    return res\n",
    "\n",
    "N = int(input())\n",
    "a = []\n",
    "for _ in range(N):\n",
    "    a.append(int(input()))\n",
    "a.sort()\n",
    "print(solution(N))"
   ]
