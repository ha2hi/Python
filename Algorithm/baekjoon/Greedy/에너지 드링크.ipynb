    "# https://www.acmicpc.net/problem/20115\n",
    "def solution(n,a):\n",
    "    a.sort(reverse=True)\n",
    "    res = a[0]\n",
    "    for i in range(1,n):\n",
    "        res += (a[i]/2)\n",
    "    \n",
    "    return res\n",
    "\n",
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "print(solution(n,a))\n"
   ]
