   "source": [
    "# https://www.acmicpc.net/problem/22864\n",
    "a, b, c, m = map(int, input().split())\n",
    "now_pirodo = 0\n",
    "res = 0\n",
    "i = 0\n",
    "while i != 24:\n",
    "    if now_pirodo + a <= m:\n",
    "        now_pirodo += a\n",
    "        res += b\n",
    "    else:\n",
    "        now_pirodo = now_pirodo-c if now_pirodo-c > 0 else 0\n",
    "\n",
    "    i += 1\n",
    "print(res)"
   ]
