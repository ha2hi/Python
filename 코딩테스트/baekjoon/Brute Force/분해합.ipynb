    "# https://www.acmicpc.net/problem/2231\n",
    "def solution(n):\n",
    "    for i in range(1, n+1):\n",
    "        total = i + sum(map(int, str(i)))\n",
    "        \n",
    "        if total == n:\n",
    "            return i\n",
    "    \n",
    "    return 0\n",
    "\n",
    "n = int(input())\n",
    "print(solution(n))"
   ]
