   "source": [
    "def solution(arr):\n",
    "    answer = max(arr)\n",
    "    while True:\n",
    "        if all(answer % i == 0 for i in arr):\n",
    "            return answer\n",
    "        answer += 1\n",
    "\n",
    "arr = [2,6,8,14]\n",
    "solution(arr)"
   ]
