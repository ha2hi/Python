   "source": [
    "# https://school.programmers.co.kr/learn/courses/30/lessons/12980\n",
    "# top - down으로 생각하기\n",
    "\n",
    "def solution(n):\n",
    "    answer = 0\n",
    "    while n != 0:\n",
    "        if n % 2 == 0:\n",
    "            n //= 2\n",
    "        else:\n",
    "            n -= 1\n",
    "            answer += 1\n",
    "    return answer\n",
    "\n",
    "n = 5\n",
    "solution(n)"
   ]
