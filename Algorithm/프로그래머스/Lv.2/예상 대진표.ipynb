    "# https://school.programmers.co.kr/learn/courses/30/lessons/12985\n",
    "def solution(n,a,b):\n",
    "    answer = 0\n",
    "    while a != b:\n",
    "        a = (a+1)//2\n",
    "        b = (b+1)//2\n",
    "        answer += 1\n",
    "    return answer\n",
    "\n",
    "n = 8\n",
    "a = 4\n",
    "b = 7\n",
    "solution(n,a,b)"
