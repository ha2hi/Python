   "source": [
    "# https://www.acmicpc.net/problem/10815\n",
    "n = int(input())\n",
    "cards = set(list(map(int, input().split())))\n",
    "m = int(input())\n",
    "res = [0] * m\n",
    "check = list(map(int, input().split()))\n",
    "\n",
    "for i in range(m):\n",
    "    if check[i] in cards:\n",
    "        res[i] = 1\n",
    "    else:\n",
    "        res[i] = 0\n",
    "print(*res)"
   ]
